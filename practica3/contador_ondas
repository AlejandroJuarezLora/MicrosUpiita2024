:vpi_module "/usr/lib/aarch64-linux-gnu/ivl/v2005_math.vpi";
:vpi_module "/usr/lib/aarch64-linux-gnu/ivl/va_math.vpi";
S_0xafce0173b8d0 .scope module, "tb_contador_8bits" "tb_contador_8bits" 2 3;
 .timescale -9 -12;
v0xafce0174e5d0_0 .var "clk", 0 0;
v0xafce0174e6a0_0 .net "count", 7 0, v0xafce0174e3d0_0;  1 drivers
v0xafce0174e770_0 .var "rst", 0 0;
S_0xafce0173ba60 .scope module, "uut" "contador_8bits" 2 11, 3 1 0, S_0xafce0173b8d0;
 .timescale 0 0;
    .port_info 0 /INPUT 1 "clk";
    .port_info 1 /INPUT 1 "rst";
    .port_info 2 /OUTPUT 8 "count";
v0xafce0173bcb0_0 .net "clk", 0 0, v0xafce0174e5d0_0;  1 drivers
v0xafce0174e3d0_0 .var "count", 7 0;
v0xafce0174e4b0_0 .net "rst", 0 0, v0xafce0174e770_0;  1 drivers
E_0xafce01702f20 .event posedge, v0xafce0174e4b0_0, v0xafce0173bcb0_0;
    .scope S_0xafce0173ba60;
T_0 ;
    %wait E_0xafce01702f20;
    %load/vec4 v0xafce0174e4b0_0;
    %flag_set/vec4 8;
    %jmp/0xz  T_0.0, 8;
    %pushi/vec4 0, 0, 8;
    %assign/vec4 v0xafce0174e3d0_0, 0;
    %jmp T_0.1;
T_0.0 ;
    %load/vec4 v0xafce0174e3d0_0;
    %addi 1, 0, 8;
    %assign/vec4 v0xafce0174e3d0_0, 0;
T_0.1 ;
    %jmp T_0;
    .thread T_0;
    .scope S_0xafce0173b8d0;
T_1 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0xafce0174e5d0_0, 0, 1;
T_1.0 ;
    %delay 5000, 0;
    %load/vec4 v0xafce0174e5d0_0;
    %inv;
    %store/vec4 v0xafce0174e5d0_0, 0, 1;
    %jmp T_1.0;
    %end;
    .thread T_1;
    .scope S_0xafce0173b8d0;
T_2 ;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0xafce0174e770_0, 0, 1;
    %delay 10000, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0xafce0174e770_0, 0, 1;
    %delay 100000, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0xafce0174e770_0, 0, 1;
    %delay 10000, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0xafce0174e770_0, 0, 1;
    %delay 200000, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0xafce0174e770_0, 0, 1;
    %delay 10000, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0xafce0174e770_0, 0, 1;
    %delay 50000, 0;
    %vpi_call 2 44 "$stop" {0 0 0};
    %end;
    .thread T_2;
    .scope S_0xafce0173b8d0;
T_3 ;
    %vpi_call 2 49 "$dumpfile", "tb_contador_8bits_result.vcd" {0 0 0};
    %vpi_call 2 50 "$dumpvars", 32'sb00000000000000000000000000000000, S_0xafce0173b8d0 {0 0 0};
    %end;
    .thread T_3;
# The file index is used to find the file name in the following table.
